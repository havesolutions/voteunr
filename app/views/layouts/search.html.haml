%html
  %head
    %title Vote Ulhasnagar
    = stylesheet_link_tag "search.css", 'jquery-ui.css', '/facebox/facebox.css'
    = javascript_include_tag "jquery.js", "/facebox/facebox.js", "application", "jquery-ui.js"
  %body
    - flash.each do |name, msg|
      = content_tag :div, msg, :id => "flash_#{name}"
    = content_tag :h1, yield(:title) if show_title?
    %table{:style => "width:100%"}
      %tr
        %td
          #left
            = image_tag("help.png")
        %td
          = yield
        %td
          #right
            = image_tag("main.png")

